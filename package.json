{
  "name": "dbgmode_website",
  "private": true,
  "version": "0.0.0",
  "devDependencies": {
    "@astrojs/react": "^1.2.2",
    "@csstools/postcss-nested-calc": "^1.0.0",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "astro": "^1.6.13",
    "autoprefixer": "^10.4.13",
    "cmdk": "^0.1.20",
    "postcss": "^8.4.19",
    "postcss-calc": "^8.2.4",
    "postcss-custom-properties": "^12.1.11",
    "postcss-html": "^1.5.0",
    "postcss-import": "^15.1.0",
    "postcss-mixins": "^9.0.4",
    "postcss-nested": "^6.0.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^2.8.1",
    "prettier-plugin-astro": "^0.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "stylelint": "^14.16.0",
    "stylelint-config-clean-order": "^2.1.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard": "^29.0.0"
  },
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro"
  },
  "stylelint": {
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-prettier",
      "stylelint-config-html/astro",
      "stylelint-config-clean-order"
    ],
    "rules": {
      "at-rule-empty-line-before": [
        "always",
        {
          "except": [
            "first-nested"
          ]
        }
      ],
      "at-rule-no-unknown": [
        true,
        {
          "ignoreAtRules": [
            "define-mixin",
            "mixin"
          ]
        }
      ],
      "unit-no-unknown": [
        true,
        {
          "ignoreUnits": [
            "--bgsq",
            "--bgl"
          ]
        }
      ],
      "order/order": [
        "dollar-variables",
        {
          "type": "at-rule",
          "name": "mixin"
        },
        "custom-properties",
        "declarations",
        {
          "type": "rule",
          "selector": "/^&::[\\w-]+/",
          "hasBlock": true
        },
        "rules",
        {
          "type": "at-rule",
          "name": "media",
          "hasBlock": true
        }
      ]
    }
  },
  "prettier": {
    "singleQuote": true,
    "jsxSingleQuote": true,
    "quoteProps": "consistent",
    "bracketSpacing": false,
    "plugins": [
      "./node_modules/prettier-plugin-astro"
    ],
    "overrides": [
      {
        "files": "*.astro",
        "options": {
          "parser": "astro"
        }
      }
    ]
  }
}
