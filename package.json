{
  "name": "dbgmode_website",
  "private": true,
  "version": "0.0.0",
  "devDependencies": {
    "@astrojs/react": "^1.0.0",
    "@csstools/postcss-nested-calc": "^1.0.0",
    "@types/react-dom": "^18.0.6",
    "astro": "^1.0.6",
    "autoprefixer": "^10.4.8",
    "cmdk": "^0.1.19",
    "postcss": "^8.4.16",
    "postcss-calc": "^8.2.4",
    "postcss-custom-properties": "^12.1.8",
    "postcss-html": "^1.5.0",
    "postcss-import": "^14.1.0",
    "postcss-mixins": "^9.0.3",
    "postcss-nested": "^5.0.6",
    "postcss-simple-vars": "^6.0.3",
    "prettier": "^2.7.1",
    "prettier-plugin-astro": "^0.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "stylelint": "^14.10.0",
    "stylelint-config-clean-order": "^0.9.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^27.0.0"
  },
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro"
  },
  "stylelint": {
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-prettier",
      "stylelint-config-html/astro",
      "stylelint-config-clean-order"
    ],
    "rules": {
      "at-rule-empty-line-before": [
        "always",
        {
          "except": [
            "first-nested"
          ]
        }
      ],
      "at-rule-no-unknown": [
        true,
        {
          "ignoreAtRules": [
            "define-mixin",
            "mixin"
          ]
        }
      ],
      "unit-no-unknown": [
        true,
        {
          "ignoreUnits": [
            "--bgsq",
            "--bgl"
          ]
        }
      ]
    }
  },
  "prettier": {
    "singleQuote": true,
    "jsxSingleQuote": true,
    "quoteProps": "consistent",
    "bracketSpacing": false,
    "plugins": [
      "./node_modules/prettier-plugin-astro"
    ],
    "overrides": [
      {
        "files": "*.astro",
        "options": {
          "parser": "astro"
        }
      }
    ]
  }
}
